{
  "name": "mongoapi",
  "version": "1.0.0",
  "description": "A mongo server",
  "license": "ISC",
  "author": "Mohan Talachutla",
  "homepage": "https://github.com/mohantalachutla/mongoapi",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mohantalachutla/mongoapi.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    }
  },
  "files": [
    "lib/"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "app": {
    "host": "http://localhost",
    "port": 3001
  },
  "scripts": {
    "start": "node lib/index.js",
    "start:dev": "nodemon --watch ./src --exec \"npm run start\"",
    "build": "webpack --mode production --config ./webpack/webpack.config.cjs ",
    "build:dev": "webpack --mode development --watch --config ./webpack/webpack.config.cjs ",
    "start:mongo": "mongo-express --url mongodb://localhost:27017/default",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:format": "prettier ./**/*.{js,mjs,cjs,json} --write .",
    "lint:staged": "lint-staged",
    "lint:all": "npm run lint:fix && npm run lint:format"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.2",
    "formidable": "^1.2.6",
    "http-errors": "^1.8.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongodb": "^4.17.2",
    "mongoose": "^5.13.14"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-transform-commonjs": "^1.1.6",
    "debug": "^4.3.2",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "mongo-express": "^1.0.0",
    "morgan": "^1.10.0",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.4.2",
    "sharp": "^0.33.5",
    "supports-color": "^9.0.2",
    "typescript": "^4.7.4",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "yarn": "^1.22.19"
  }
}
