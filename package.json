    "@rollup/plugin-terser": "^0.4.4",
    "debug": "^4.3.2",
    "eslint": "^9.16.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "mongo-express": "^1.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.9",
    "prettier": "^3.4.2",
    "rollup": "^4.28.1",
    "sharp": "^0.33.5",
    "supports-color": "^9.0.2",
    "typescript": "^4.7.4",
    "yarn": "^1.22.19"
  }
